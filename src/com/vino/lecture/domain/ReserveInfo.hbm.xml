<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="com.vino.lecture.domain.ReserveInfo" table="reserveinfo">
    		<id name="id" column="id">
    			<generator class="native"></generator>
    		</id>
    		<property name="name" column="name"></property>
    		<property name="username" column="username"></property>
    		<property name="lectureId" column="lectureId"></property>
    		<property name="attence" column="attence"></property>
    		<property name="grade" column="grade"></property>
    		<property name="major" column="major"></property>
    	</class>
    </hibernate-mapping>